    draw_rectangle(v_x, room_height, v_x + v_width, v_y + v_height, false);
    
    draw_set_alpha(1);
}

if (instance_number(obj_bat) &lt;= 0) {
    draw_set_color(c_black);
    draw_set_alpha(0.5);
    
    draw_rectangle(v_x, v_y, v_x + v_width, v_y + v_height, false);
    
